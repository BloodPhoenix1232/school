#include <stdio.h>

#define NMAX 10

int input(int *buffer, int *length, int *shift);
void output(int *buffer, int length);
void cycle_shift(int *buffer, int length, int shift);

int main()
{
    int buffer[NMAX];
    int length, shift;
    
    if (input(buffer, &length, &shift) != 0) {
        printf("n/a");
        return 1;
    }
    
    cycle_shift(buffer, length, shift);
    
    output(buffer, length);
    
    return 0;
}

int input(int *buffer, int *length, int *shift)
{
    if (scanf("%d", length) != 1) {
        return 1;
    }
    
    if (*length <= 0 || *length > NMAX) {
        return 1;
    }
    
    for (int i = 0; i < *length; i++) {
        if (scanf("%d", &buffer[i]) != 1) {
            return 1;
        }
    }
    
    if (scanf("%d", shift) != 1) {
        return 1;
    }
    
    return 0;
}

void output(int *buffer, int length)
{
    for (int i = 0; i < length; i++) {
        printf("%d", buffer[i]);
        if (i < length - 1) {
            printf(" ");
        }
    }
}

void cycle_shift(int *buffer, int length, int shift)
{
    if (length == 0) return;
    
    // Нормализуем сдвиг
    shift = shift % length;
    if (shift < 0) {
        // Преобразуем сдвиг вправо в эквивалентный сдвиг влево
        shift = length + shift;
    }
    
    // Создаем временный массив для хранения сдвинутых элементов
    int temp[NMAX];
    
    // Копируем элементы в новый массив со сдвигом
    for (int i = 0; i < length; i++) {
        temp[(i + shift) % length] = buffer[i];
    }
    
    // Копируем обратно в исходный массив
    for (int i = 0; i < length; i++) {
        buffer[i] = temp[i];
    }
}
